{
    "name": "mdantas/expressive-cli",
    "description": "This package is another tooling to expressive framework.",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.2@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Dantas",
            "email": "marcos.adantas@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/console": "^4.3@dev",
        "mdantas/phinx-wrapper": "dev-master",
        "psr/container": "^1.0@dev",
        "zendframework/zend-expressive-tooling": "^1.2@dev"
    },
    "autoload": {
        "psr-4": {
            "mdantas\\ExpressiveCli\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "ConfigProvider": "mdantas\\ExpressiveCli\\ConfigProvider"
        }
    },
    "bin": [
        "bin/expressive-cli"
    ]
}
